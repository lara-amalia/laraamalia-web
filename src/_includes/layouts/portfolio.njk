{% extends "layouts/base-page.njk" %}

{% set metaTitle = web[locale].portfolio.meta.title %}
{% set metaDescription = web[locale].portfolio.meta.description %}
{% set pageTitle = title %}

{% block content %}
<div class="text-section">
  <p>{{ web[locale].portfolio.intro | safe }}</p>
</div>
<div class="grid-base portfolio-grid">
  <!-- PROJECT: TimeSloth -->
  <article class="project project-timesloth">
    <div class="text-section">
      <h2>{{ web[locale].portfolio.projects.timesloth.title }}</h2>
      <p>
        {{ web[locale].portfolio.projects.timesloth.description }}<br />
        <span class="project-technology"
          >{{ web[locale].portfolio.projects.timesloth.technology }}</span
        ><br />
        {{ web[locale].portfolio.projects.timesloth.time }}
      </p>

      <a
        class="project-link nav-link"
        href="{{ web[locale].portfolio.projects.timesloth.link }}"
        target="_blank"
        rel="noopener noreferrer"
        title="{{ t.common.goToWebsite }}"
        >{{ t.common.projectLink }}</a
      >
    </div>
  </article>
  <a
    class="project-img project-timesloth-img"
    href="{{ web[locale].portfolio.projects.timesloth.link }}"
    target="_blank"
    rel="noopener noreferrer"
    title="{{ t.common.goToWebsite }}"
  >
    <div class="project-img-wrapper">
      <img
        src="../assets/images/portfolio/timesloth-700.jpg"
        srcset="
          ../assets/images/portfolio/timesloth-700.jpg  1x,
          ../assets/images/portfolio/timesloth-1400.jpg 2x
        "
        alt="TimeSloth Website Screenshot"
      />
    </div>
  </a>
  <div aria-hidden="true" class="bg-blue-base bg-timesloth"></div>

